{
  "name": "tests-always-included/soapy",
  "description": "A PHP SoapClient inspired by Guzzle and Symfony",
  "type": "library",
  "keywords": ["SOAP", "SoapClient"],
  "license": "MIT",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/"
    }
  },
  "require": {
    "php": ">=5.3",
    "ext-soap": "*",
    "symfony/event-dispatcher": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "@dev"
  },
  "suggest": {
    "monolog/monolog": "Log requests and responses."
  },
  "support": {
    "issues": "https://github.com/wjaspers/soapy/issues",
    "source": "https://github.com/wjaspers/soapy"
  }
}
